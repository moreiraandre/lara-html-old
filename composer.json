{
    "name": "moreiraandre/php-html",
    "description": "Pacote Laravel para gerar marcação HTML com abstração do sistema de grids responsivo.",
    "license": "MIT",
    "keywords": ["laravel", "php", "html", "responsive", "grid", "row", "column"],
    "authors": [
        {
            "name": "André Moreira",
            "email": "andre.mcx1+lara-html@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Tom Benevides",
            "email": "eltonfelixbenevides+lara-html@gmail.com",
            "role": "Documentation manager"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "LaraHtml\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraHtml\\LaraHtmlProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
